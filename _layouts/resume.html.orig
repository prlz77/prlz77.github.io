  <table>
    {% for study in site.data.resume.studies %}
      <tr>
        <td class="first-col" style="width: 15%; border-right:2px solid black;">{{ study.when }}</td>
        <td class="second-col"><h4 style='color=rgba(0,0,0,1); display:inline;'>{{ study.name }}</h4><br>
          <b>{{ study.where }}</b><br>
          {{ study.description }}</td>
      </tr>
    {% endfor %}
  </table>
  <h3><i class="fa fa-briefcase"></i> Work Experience</h3>
  <hr style="margin-top:0px;">
  <table>
    {% for work in site.data.resume.work %}
      <tr>
        <td class="first-col" style="width: 15%; border-right:2px solid black;">{{ work.when }}</td>
        <td class="second-col"><h4 style='display:inline;'>{{ work.name }}</h4><br>
          <b>{{ work.where }}</b><br>
          {{ work.description }}</td>
      </tr>
    {% endfor %}
  </table>
  <h3><i class="fa fa-book"></i> Projects</h3>
  <hr style="margin-top:0px;">
  <table>
    {% for project in site.data.resume.projects %}
      <tr>
        <td class="first-col" style="width: 15%; border-right:2px solid black;">{{ project.when }}</td>
        <td class="second-col"><h4 style='display:inline;'>{{ project.name }}</h4><br>
          <b>{{ project.where }}</b><br>
